<config>
    <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
       <interface>
          <GigabitEthernet>
             <name>{int_name}</name>
             <shutdown xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0" nc:operation="delete"/>
             <ip>
                 <address>
                    <primary>
                         <address>{ip_address}</address>
                             <mask>{subnet_mask}</mask>
                    </primary>
                 </address>
           </ip>
          </GigabitEthernet>
        </interface>
      </native>
</config>
